<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lowelostudents.caloriecounter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CalorieCounter">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/activity_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.lowelostudents.caloriecounter.ui.models.CreateMeal"
            android:label="@string/title_activity_create_meal" />

        <activity
            android:name="com.lowelostudents.caloriecounter.ui.models.CreateFood"
            android:label="@string/title_activity_create_food" />

<!--        <activity-->
<!--            android:name="com.lowelostudents.caloriecounter.ui.CRUDActivity"-->
<!--            android:label="@string/title_activity_create_food" />-->
    </application>

</manifest>